{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "react-typescript",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "react-typescript": {
      "root": "apps/react-typescript",
      "sourceRoot": "apps/react-typescript/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired build",
            "cwd": "apps/react-typescript",
            "outputPath": "dist/apps/react-typescript"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired start",
            "cwd": "apps/react-typescript"
          }
        },
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/eslint src/**/*.tsx src/**/*.ts",
            "cwd": "apps/react-typescript"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired test --watchAll=false",
            "cwd": "apps/react-typescript"
          }
        }
      }
    },
    "react-typescript-e2e": {
      "root": "apps/react-typescript-e2e",
      "sourceRoot": "apps/react-typescript-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx serve react-typescript",
              "nx e2e-run-when-ready react-typescript-e2e"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/react-typescript-e2e/**/*.{js,ts}"
            ]
          }
        },
        "e2e-run": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-typescript-e2e/cypress.json",
            "tsConfig": "apps/react-typescript-e2e/tsconfig.e2e.json",
            "baseUrl": "http://localhost:3000"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-typescript:serve:production"
            }
          }
        },
        "e2e-run-when-ready": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx e2e-run react-typescript-e2e"
            ],
            "readyWhen": "http://localhost:3000"
          }
        }
      }
    }
  }
}